insert into products_description (products_id, language_id, products_name, products_description, products_url, products_seo_url, products_youtube_url, products_viewed) values ('2', '2', 'Matrox G400 32 MB', '<b>Neu! Matrox G400 &quot;all inclusive&quot; und vieles mehr...</b><br><br>
