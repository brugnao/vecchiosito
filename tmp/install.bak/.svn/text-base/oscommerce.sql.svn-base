insert into products_description (products_id, language_id, products_name, products_description, products_url, products_seo_url, products_youtube_url, products_viewed) values ('16', '2', 'Mut Zur Wahrheit', 'Originaltitel: &quot;Courage Under Fire&quot;<br><br>
