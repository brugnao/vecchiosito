insert into products_description (products_id, language_id, products_name, products_description, products_url, products_seo_url, products_youtube_url, products_viewed) values ('21', '2', 'SWAT 3: Elite Edition', '<b>KEINE KOMPROMISSE!</b><br><i>Kmpfen Sie Seite an Seite mit Ihren LAPD SWAT-Kameraden gegen das organisierte Verbrechen!</i><br><br>
